  <servlet-class>com.sunbeam.servlets.LoginServlet</servlet-class>
  
 </servlet>
 
 		<servlet-mapping>
 
				 <servlet-name>Login</servlet-name>
				 <url-pattern>/login</url-pattern>
				 
 
		 </servlet-mapping>
		 
		 
